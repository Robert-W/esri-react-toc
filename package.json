{
  "name": "esri-react-toc",
  "version": "0.1.0",
  "description": "TOC widget for Esri's 4.0 javascript API.",
  "main": "gulpfile.js",
  "scripts": {
    "start": "npm run babel -- -w & gulp start",
    "babel": "babel src --out-dir build",
    "postinstall": "bower install",
    "test": "flow check;eslint src/toc/*.js;",
    "dist": "rm -rf dist;BABEL_ENV=production node ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robert-W/esri-react-toc.git"
  },
  "keywords": [
    "esri",
    "react",
    "flexbox",
    "toc",
    "arcgis",
    "javascript",
    "4.0"
  ],
  "author": "Robert-W",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Robert-W/esri-react-toc/issues"
  },
  "homepage": "https://github.com/Robert-W/esri-react-toc#readme",
  "dependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-plugin-transform-es2015-modules-amd": "^6.6.5",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bower": "^1.7.9",
    "flow-bin": "^0.24.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015-rollup": "^1.1.1",
    "browser-sync": "^2.12.3",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.3.0",
    "gulp": "^3.9.1",
    "rollup": "^0.31.2",
    "rollup-plugin-babel": "^2.5.1",
    "uglify-js": "^2.6.2"
  }
}
